<?xml version="1.0"?>
<rtde_config>
	<recipe key="state">
    <field name="timestamp" type="DOUBLE"/>
    <field name="robot_mode" type="INT32"/>
		<field name="target_q" type="VECTOR6D"/>
		<!-- <field name="target_qd" type="VECTOR6D"/> -->
    <!-- <field name="target_qdd" type="VECTOR6D"/> -->
		<field name="actual_q" type="VECTOR6D"/> <!-- Actual joint positions -->
		<field name="actual_qd" type="VECTOR6D"/> <!-- Actual joint velocities -->
    <!-- <field name="target_current" type="VECTOR6D"/> Target joint currents -->
    <!-- <field name="target_moment" type="VECTOR6D"/>  Target joint moments (torques) -->
		<!-- <field name="actual_current" type="VECTOR6D"/> Actual joint currents -->
		<!-- <field name="joint_control_output" type="VECTOR6D"/>Joint control currents -->
		<field name="target_TCP_pose" type="VECTOR6D"/> <!-- Target Cartesian coordinates of the tool: (x,y,z,rx,ry,rz), where rx, ry and rz is a rotation vector representation of the tool orientation -->
		<!-- <field name="target_TCP_speed" type="VECTOR6D"/>  Target speed of the tool given in Cartesian coordinates. The speed is given in [m/s] and the rotational part of the TCP speed (rx, ry, rz) is the angular velocity given in [rad/s] -->
		<field name="actual_TCP_pose" type="VECTOR6D"/> <!-- Actual Cartesian coordinates of the tool: (x,y,z,rx,ry,rz), where rx, ry and rz is a rotation vector representation of the tool orientation -->
		<field name="actual_TCP_speed" type="VECTOR6D"/> <!-- Actual speed of the tool given in Cartesian coordinates. The speed is given in [m/s] and the rotational part of the TCP speed (rx, ry, rz) is the angular velocity given in [rad/s] -->
		<!-- <field name="actual_TCP_force" type="VECTOR6D"/> Generalized forces in the TCP. It compensates the measurement for forces and torques generated by the payload -->
		<field name="output_int_register_0" type="INT32"/>
	</recipe>

	<!-- <recipe key="urOpt"0> -->
	<!-- </recipe> -->


	<recipe key="setp">
		<field name="input_double_register_0" type="DOUBLE"/>
		<field name="input_double_register_1" type="DOUBLE"/>
		<field name="input_double_register_2" type="DOUBLE"/>
		<field name="input_double_register_3" type="DOUBLE"/>
		<field name="input_double_register_4" type="DOUBLE"/>
		<field name="input_double_register_5" type="DOUBLE"/>
		<field name="input_int_register_1" type="INT32"/>
	</recipe>

		<!-- <field name="input_integer_register_1" type="DOUBLE"/> -->

	<recipe key="watchdog">
		<field name="input_int_register_0" type="INT32"/>
	</recipe>
</rtde_config>
